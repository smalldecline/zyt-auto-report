                        cook = "";
                    }
                    if (cook == "turn") {
                        mui.alert("Please choose 1 questions!");
                        $(".mui-popup-title").html("Attention");
                        $(".mui-popup-button-bold").html("Ok");
                        $("#btn_save").attr("disabled", false);

                    } else {
                        mui.alert("请选择1题！");
                        $("#btn_save").attr("disabled", false);
                    }

                    return false;

                }

                if (DATA_2 != '正常' || DATA_3 == '体温异常' || DATA_4 != '否' || DATA_5 != '绿码' || DATA_6 != '否' || DATA_7 != '否' || DATA_8 != '否' || DATA_10 != '否' || DATA_16 != '否') {
