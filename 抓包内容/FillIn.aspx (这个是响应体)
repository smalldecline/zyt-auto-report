                        var url = window.location.search;
                        // 正则筛选地址栏
                        var reg = new RegExp("(^|&)ENT=([^&]*)(&|$)");
                        // 匹配目标参数
                        var result = url.substr(1).match(reg);
                        try {
                            var cook = decodeURIComponent(result[2])
                        } catch (e) {
                            cook = "";
                        }
                        if (cook == "turn") {
                            mui.alert("Questions 1-11 include “abnormal temperature” or “abnormal”, “non Green Code” and “yes”. Question 12 cannot be blank");
                            $(".mui-popup-title").html("Attention");
                            $(".mui-popup-button-bold").html("Ok");
                            $("#btn_save").attr("disabled", false);
                        } else {
                            mui.alert("第1-11题有选择“体温异常”或“非正常”，“非绿码”，“是”选项，第12题不能为空");
                            $("#btn_save").attr("disabled", false);
                        }
                        return false;
                    }
                }
                
                if (DATA_13 == '' || typeof (DATA_13) == "undefined") {
                    var url = window.location.search;
                    // 正则筛选地址栏
