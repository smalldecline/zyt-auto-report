    <script src="../mui/js/mui.picker.js"></script>
    <script src="../mui/js/mui.poppicker.js"></script>
    <script src="../mui/js/city.data-3.js?20200406" type="text/javascript" charset="utf-8"></script>
    <script type="text/javascript">
        (function ($, doc) {
            $.init();
            $.ready(function () {
                var cityPicker3 = new $.PopPicker({
                    layer: 3
                });
                cityPicker3.setData(cityData3);
                var showCityPicker = doc.getElementById('DATA_14');
                showCityPicker.addEventListener('tap', function (event) {
                    cityPicker3.show(function (items) {
                        var addres = (items[0] || {}).text + " " + (items[1] || {}).text + " " + (items[2] || {}).text;
                        document.getElementById(showCityPicker.id).value = addres;

                    });
                }, false);
            });
        })(mui, document);
        // 获取url参数
        var url = window.location.search;
        // 正则筛选地址栏
        var reg = new RegExp("(^|&)ENT=([^&]*)(&|$)");
        // 匹配目标参数
        var result = url.substr(1).match(reg);
        try {
            var cook = decodeURIComponent(result[2])
        } catch (e) {
            cook = "";
        }

        if (cook == "turn") {
            $(".Ent").show();
            $(".Entfalse").hide();
            $("#DATA_11").attr("placeholder", "Please fill in the text")
            $("#DATA_12").attr("placeholder", "Please fill in the text")
            $("#DATA_14").attr("placeholder", "Please select")
            document.title = "Daily Check-in";
        } else {
            $(".Ent").hide();
        }

        $("#return").click(function () {
            try {
                var cook = decodeURIComponent(result[2])
            } catch (e) {
                cook = "";
            }
            if (cook == "turn") {
                $(".Ent").show();
                window.location.href = '/H5/ZJSFDX/PunchCardRecord.aspx?ENT=turn';
            } else {
                window.location.href = '/H5/ZJSFDX/PunchCardRecord.aspx';
            }
        })
    </script>
