        var cj = {
            getUrlParam: function (key, defvalue) {
                // 获取url参数
                var url = window.location.search;
                // 正则筛选地址栏
                var reg = new RegExp("(^|&)" + key + "=([^&]*)(&|$)");
                // 匹配目标参数
                var result = url.substr(1).match(reg);
                //返回参数值
                if (defvalue != "" && typeof (defvalue) != "undefined") {
                    return result ? decodeURIComponent(result[2]) : defvalue;
                } else {
                    return result ? decodeURIComponent(result[2]) : '';
                }
            },
        }


        function remind(str) {
            mui.init({
                swipeBack: true //启用右滑关闭功能
            });

            try {
                var cook = cj.getUrlParam("ENT")
            } catch (e) {
                cook = "";
            }

            if (cook == "turn") {
                var btnArray = ['Ok'];
                mui.confirm(str, 'Attention', btnArray, function (e) {
                    if (e.index == 0) {
                        $(".Ent").show();
                        var myDate = new Date;
                        var year = myDate.getFullYear(); //获取当前年
                        var mon = myDate.getMonth() + 1; //获取当前月
                        var date = myDate.getDate(); //获取当前日
                        console.log(getCookie('TimeDay') + "," + date + "," + getCookie('LastUserCode'))
                        setCookie('TimeDay', date, 180);
                        window.location.href = '/H5/ZJSFDX/PunchCardRecord.aspx?ENT=turn&Temperature=新版';
                    }
                })
            } else {
                var btnArray = ['是'];
                mui.confirm(str, '提示', btnArray, function (e) {
                    if (e.index == 0) {
                        var myDate = new Date;
                        var year = myDate.getFullYear(); //获取当前年
                        var mon = myDate.getMonth() + 1; //获取当前月
                        var date = myDate.getDate(); //获取当前日
                        console.log(getCookie('TimeDay') + "," + date + "," + getCookie('LastUserCode'))
                        setCookie('TimeDay', date, 180);

                        window.location.href = '/H5/ZJSFDX/PunchCardRecord.aspx?Temperature=新版';
                    }
                })
            }
        }

        $(function () {
            var address = cj.getUrlParam("address");
